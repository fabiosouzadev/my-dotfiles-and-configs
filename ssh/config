# Azure
Host ssh.dev.azure.com
  HostName ssh.dev.azure.com
  PreferredAuthentications publickey
  IdentitiesOnly yes

# Github
Host github.com
  Hostname ssh.github.com
  Port 443
  User git
  PreferredAuthentications publickey
# Bitbucket
Host bitbucket.org
  Hostname altssh.bitbucket.org
  Port 443

#Lembrar senhas
Host *
  AddKeysToAgent yes
  UseKeychain yes
